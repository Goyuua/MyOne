                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <!--配置编译级别和编码格式-->
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <encoding>UTF-8</encoding>
                </configuration>
